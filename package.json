{
  "name": "pendo-guide",
  "version": "1.0.0",
  "scripts": {
    "start": "npm-run-all --parallel styles:watch postcss browsersync",
    "styles:watch": "node-sass --watch scss/mri-ui.scss css/mri-ui.css",
    "postcss": "postcss css/mri-ui.css --replace --use autoprefixer",
    "browsersync": "browser-sync start --server --files \"*.html, *.js, css/*.css\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aos": "^2.3.4",
    "browser-sync": "^2.26.13",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.1",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1"
  }
}
